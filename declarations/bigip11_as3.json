{
   "$schema": "https://raw.githubusercontent.com/F5Networks/f5-appsvcs-extension/main/schema/latest/as3-schema.json",
   "class": "AS3",
   "action": "deploy",
   "persist": true, 
   "logLevel": "warning",
   "declaration": {
      "class": "ADC",
      "schemaVersion": "3.0.0",
      "id": "urn:uuid:33045210-3ab8-4636-9b2a-c98d22ab915d", 
      "label": "Sample 1",
      "remark": "Simple HTTP application with RR pool",
      "updateMode": "selective", 
      "Partition-1": { 
         "class": "Tenant",
         "defaultRouteDomain": 0,
         "enable": true, 
         "A1": { 
            "class": "Application",
            "vs_Uptime_Kuma_v4_tcp3001": {
               "class": "Service_HTTP",
               "virtualAddresses": [
                  "198.18.81.8"
               ],
               "virtualPort": 3001,
               "virtualType": "standard",
               "adminState": "enable",
               "enable": true, 
               "shareAddresses": false, 
               "remark": "Virtual Server Uptime Kuma v4",
               "persistenceMethods": [],
               "profileTCP": {"bigip":"/Common/f5-tcp-progressive"},
               "profileHTTP": "basic",
               "snat": "auto",
               "allowVlans": ["vmnet1"],
               "pool": "pl_Uptime_Kuma_v4"
            },
            "vs_Uptime_Kuma_v4_tcp43001": {
               "class": "Service_HTTPS",
               "virtualAddresses": [
                  "198.18.81.8"
               ],
               "virtualPort": 43001,
               "virtualType": "standard",
               "adminState": "enable",
               "enable": true, 
               "shareAddresses": false, 
               "remark": "Virtual Server Uptime Kuma v4 TLS",
               "serverTLS": "client_ssl_webtls",
               "persistenceMethods": [],
               "profileTCP": {"bigip":"/Common/f5-tcp-progressive"},
               "profileHTTP": "basic",
               "snat": "auto",
               "redirect80": false,
               "allowVlans": ["vmnet1"],
               "pool": {"use": "pl_Uptime_Kuma_v4"}
            },
            "client_ssl_webtls": {
               "class": "TLS_Server",
               "certificates": [{
                  "certificate": "acmeCert"
                }]
             },
             "acmeCert": {
               "class": "Certificate",
               "remark": "In practice we recommend using a passphrase",
               "certificate": "-----BEGIN CERTIFICATE-----\\n-----END CERTIFICATE-----",
               "privateKey": "-----BEGIN PRIVATE KEY-----\\n-----END PRIVATE KEY-----"
             },
            "pl_Uptime_Kuma_v4": {
               "class": "Pool",
               "minimumMembersActive": 0,
               "minimumMonitors": "all",
               "monitors": [
                  "http"
               ],
               "members": [{
                  "enable": true, 
                  "adminState": "enable",
                  "servicePort": 3001,
                  "shareNodes": true,
                  "servers": [{
                    "name": "nd_unifi-console", 
                    "address": "198.18.1.8"
                    }]
               }
               ]
            }
         }
      }
   }
}
